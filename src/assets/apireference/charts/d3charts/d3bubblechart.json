{
    "name": "Amexio D3-chart Bubble",
    "description": "An  D3-chart Bubble chart that is rendered within the browser using SVG .Displays tips when hovering over points.",
    "selector": "<amexio-d3-chart-bubble>",
    "properties": [
        {
            "name": "title",
            "version": "0.0.7 Onwards",
            "description": "Sets title to chart",
            "type": "string",
            "default": ""
        },
        {
            "name": "width",
            "description": "Width of chart",
            "version": "0.0.7 Onwards",
            "type": "string",
            "default": ""
        },
        {
            "name": "height",
            "description": "Height of chart",
            "version": "0.0.7 Onwards",
            "type": "string",
            "default": ""
        },
        {
            "name": "data",
            "description": "Local data for Chart",
            "version": "0.0.7 Onwards",
            "type": "any[]",
            "default": ""
        },
        {
            "name": "legend",
            "description": "Sets legend to D3 Bubble chart",
            "version": "0.0.7 Onwards",
            "type": "boolean",
            "default": "true"
        },
        {
            "name": "http-url",
            "description": "REST url for fetching data.",
            "version": "0.0.7 Onwards",
            "type": "string",
            "default": ""
        },
        {
            "name": "http-method",
            "description": "Type of HTTP call, POST,GET etc.",
            "version": "0.0.7 Onwards",
            "type": "string",
            "default": ""
        },
        {
            "name": "data-reader",
            "description": "Key in JSON Datasource for records.",
            "version": "0.0.7 Onwards",
            "type": "string",
            "default": ""
        },
        {
            "name": "level",
            "description": "Sets level for chart when drillable property used",
            "version": "0.0.7 Onwards",
            "type": "number",
            "default": ""
        },
        {
            "name": "target",
            "description": "Sets  target for chart when drillable property used",
            "version": "0.0.7 Onwards",
            "type": "number",
            "default": ""
        },
        {
            "name": "drillable-data",
            "description": "Arrays of keys provided for chart, when drillable property used.",
            "version": "0.0.7 Onwards",
            "type": "any",
            "default": ""
        },  {
            "name": "horizontal-scale",
            "description": "Sets Horizontal scaling ",
            "version": "0.0.7 Onwards",
            "type": "boolean",
            "default": "true"
        },  {
            "name": "vertical-scale",
            "description": "Sets Vertical scaling ",
            "version": "0.0.7 Onwards",
            "type": "boolean",
            "default": "true"
        }
    ],
  
    "chartevent": [
        {
            "name": "onChartClick()",
            "version": "0.0.7 Onwards",
            "description": "It will fire only on click on chart in D3 Bubble chart ."
        },

        {
            "name": "onLegendClick()",
            "version": "0.0.7 Onwards",
            "description": "It will fire only on click on Legend Click in D3 Bubble chart ."
        }
    ]
}