{
  "name": "Amexio Label",
  "description": "Amexio Label can be easily wrapped around any text and configure using the different responsive styling",
  "selector": "<'amexio-label>",
  "properties": [
    {
      "name": "badge",
      "description": "Badge Value for Label.",
      "version": "4.1.9 onwards",
      "type": "number",
      "default": ""
    },
    {
      "name": "enable-click",
      "description": "Enable label click",
      "version": "4.2.7 onwards",
      "type": "boolean",
      "default": "false"
    },
    {
      "name": "size",
      "description": "Responsive Font size, large,medium,small & large-bold,medium-bold,small-bold",
      "version": "4.0 onwards",
      "type": "string",
      "default": "small"
    },
    {
      "name": "font-color",
      "description": "Font color of label",
      "version": "4.0 onwards",
      "type": "string",
      "default": "small"
    }
  ],
  "events": [
    {
      "name": "onClick",
      "description": "Fires on click of label"
    }
  ]
}
