{
    "name": "Amexio Number Input",
    "description": "Number input component has been created with different configurable attributes for validation (min/max value, allow blank, custom regex), custom error message, help, custom styles",
    "selector": "<amexio-number-input>",
    "properties": [
      {
        "name": "field-label",
        "description": "The label of this field",
        "version": "4.0 onwards",
        "type": "string",
        "default": ""
      },
      {
        "name": "allow-blank",
        "description": "Sets if field is required",
        "version": "4.0 onwards",
        "type": "string",
        "default": ""
      },
      {
        "name": "min-value",
        "description": "Defines the min range limit for number input.",
        "version": "4.0 onwards",
        "type": "number",
        "default": ""
      },
      {
        "name": "max-value",
        "description": "Defines the max range limit for number input.",
        "version": "4.0 onwards",
        "type": "number",
        "default": ""
      },
      {
        "name": "error-msg",
        "description": "Sets the error message",
        "version": "4.0 onwards",
        "type": "string",
        "default": ""
      },
      {
        "name": "min-error-msg",
        "description": "Sets the error message for min validation",
        "version": "4.0 onwards",
        "type": "string",
        "default": ""
      },
      {
        "name": "max-error-msg",
        "description": "Sets the error message for max validation",
        "version": "4.0 onwards",
        "type": "string",
        "default": ""
      },
      {
        "name": "place-holder",
        "description": "Show place-holder inside dropdown component",
        "version": "4.0 onwards",
        "type": "string",
        "default": ""
      },
      {
        "name": "min-length",
        "description": "The smallest positive representable number -that is, the positive number closest to zero (without actually being zero). The smallest negative representable number is -min-length.",
        "version": "4.0 onwards",
        "type": "number",
        "default": ""
      },
      {
        "name": "max-length",
        "description": "The smallest positive representable number -that is, the positive number closest to zero (without actually being zero). The smallest negative representable number is -max-length.",
        "version": "4.0 onwards",
        "type": "number",
        "default": ""
      },
      {
        "name": "disabled",
        "description": "True to disable the field.",
        "version": "4.0 onwards",
        "type": "boolean",
        "default": "false"
      },
      {
        "name": "font-style",
        "description": "Set font-style to field",
        "version": "4.0 onwards",
        "type": "string",
        "default": ""
      },
      {
        "name": "font-family",
        "description": "Set font-family to field",
        "version": "4.0 onwards",
        "type": "string",
        "default": ""
      },
      {
        "name": "font-size",
        "description": "Set font-size to field",
        "version": "4.0 onwards",
        "type": "string",
        "default": ""
      },
      {
        "name": "has-label",
        "description": "Flag to set label",
        "version": "4.0 onwards",
        "type": "boolean",
        "default": "false"
      },
      {
        "name": "pattern",
        "description": "Apply Reg-ex to the field",
        "version": "4.0 onwards",
        "type": "string",
        "default": ""
      },
      {
        "name": "enable-popover",
        "description": "Set enable / disable popover.",
        "version": "4.0 onwards",
        "type": "string",
        "default": ""
      },
      {
        "name": "icon-feedback",
        "description": "Enables / disables icon feedback",
        "version": "4.0 onwards",
        "type": "boolean",
        "default": ""
      }
    ],
    "events": [
      {
        "name": "input",
        "description": "On input event field."
      }
    ]
  }
